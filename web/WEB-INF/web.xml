<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>MainJsp</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>all</servlet-name>
        <servlet-class>Browse.FileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>static</servlet-name>
        <servlet-class>org.apache.catalina.servlets.DefaultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainJsp</servlet-name>
        <url-pattern>/main.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>all</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>*.css</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>static</servlet-name>
        <url-pattern>/lib/image/favicon.png</url-pattern>
        <url-pattern>/lib/css/index.css</url-pattern>
        <url-pattern>/lib/css/font-awesome.min.css</url-pattern>
        <url-pattern>/lib/css/ionicons.min.css</url-pattern>
        <url-pattern>/lib/css/material-design-iconic-font.min.css</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.eot</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.ttf</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.svg</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.woff</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.woff2</url-pattern>
        <url-pattern>/lib/fonts/fontawesome-webfont.otf</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.eot</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.ttf</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.svg</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.woff</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.woff2</url-pattern>
        <url-pattern>/lib/fonts/Material-Design-Iconic-Font.otf</url-pattern>
        <url-pattern>/lib/fonts/ionicons.eot</url-pattern>
        <url-pattern>/lib/fonts/ionicons.ttf</url-pattern>
        <url-pattern>/lib/fonts/ionicons.svg</url-pattern>
        <url-pattern>/lib/fonts/ionicons.woff</url-pattern>
        <url-pattern>/lib/fonts/ionicons.woff2</url-pattern>
    </servlet-mapping>

</web-app>